[Domain]
    nx = 64
    ny = 64
    nz = 64
    dx = 1
    dy = 1
    dz = 1
[]

[Time]
    nstep = 1000
    dt = 0.1
[]

[Output]
    numOutputs = 10
    iskip = 1
    jskip = 1
    kskip = 1
[]

[PDApp]
    N = 64              # number of particles
    drag_coef = 3.0     # drag strength due to viscosity of medium
    bm_str = 0.5        # brownian motion strength
    [./initial_condition]
    [./inter_particle_forces]
        rcut = 30
[]

[PFApp]
    app = true
[]
